

@using HelloAngular2.Models
@model List<Electricity>

@{
    ViewBag.Title = "Electricity";
}

@if (Model.Count > 0)
{
    <table style="border:solid 1px">
        <thead>
            <tr>
                <th>Contract Name</th>
                <th>House Type</th>
                <th>Price</th>
            </tr>
        </thead>
        <tbody>
            @foreach (Electricity contract in Model)
            {
                <tr>
                    <td>@contract.ContractName</td>
                    <td>@contract.HouseType </td>
                    <td>@contract.Price EUR</td>
                </tr>
            }
          
        </tbody>
    </table>
}
else
{
    <h2>No data</h2>
}
